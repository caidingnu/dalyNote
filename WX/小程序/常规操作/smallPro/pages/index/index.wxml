<button style='margin-top:10rpx' type='primary' bindtap='toList'>跳转列表页面</button>
<text>{{test}}</text>
<button bindtap='getData' id='btn1' data-ccc='周杰伦'>周杰伦</button> 
<button bindtap='getData' id='btn2' data-ccc='刘德华'>刘德华</button> 
</view>
