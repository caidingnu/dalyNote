<!--index.wxml-->
<view class="container">
<form bindsubmit='formSubmit' bindreset='rest'>
    账号：<input type='text' name='username'  placeholder='输入账号' value='1'></input>
     密码：<input type='password' placeholder='密码' value='123'></input>
    电话： <input type='text' name='pwd'  placeholder='电话'  value='666666'></input>
   <button class='btn1' open-type='openSetting' form-type='submit'>
    <image class='btnImg' src='../img/b.png'></image>
